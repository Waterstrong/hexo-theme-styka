<% if (is_archive() || is_tag() || is_category()) { %>
    <button class="hide-labels">TAG 开关</button>
    <script src="/js/require-jquery.min.js"></script>
    <script>
        $(document).ready(function() {
            $("button").click(function() {
                $(".article-info").toggle(200);
            });
        });
    </script>
<% } %>